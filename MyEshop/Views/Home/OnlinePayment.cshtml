
@{
    ViewData["Title"] = "نتیجه پرداخت";
}



@if (ViewBag.code == "پرداخت ناموفق")
{
    <div class="col-lg-12">
        <div class="bs-component">
            <div class="alert alert-danger">
                <strong>@ViewBag.code</strong> 
            </div>
        </div>
    </div>
}
else
{
    <div class="col-lg-12">
        <div class="bs-component">
            <div class="alert alert-success">
                <strong>پرداخت موفق</strong> <p>شماره پیگیری : @ViewBag.code </p>
            </div>
        </div>
    </div>
}